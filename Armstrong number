import java.util.*;
public class Test 
{
    public static void main(String[] args) 
    {
        int sum=0, r, number =153;
        int original = number;
        while (number != 0) 
        {
            r = number % 10;
            sum=sum+(r*r*r);
            number /= 10;
        }
        if (original == sum)
           System.out.println("Armstrong Number");
        else
           System.out.println("Not Armstrong Number");
    }
}
