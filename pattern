public class NumberPattern {
    public static void main(String[] args) {
        int rows = 4;
        int number = 1;

        for (int i = 1; i <= rows; i++) {
            int square = number * number;
            for (int j = 1; j <= i; j++) {
                System.out.print(square + " ");
                square = (number + 1) * (number + 1);
                number++;
            }
            System.out.println();
        }
    }
}
