public class fact {
    public static void main(String[] args) {
        int num = 4; 
        int fact = fact(num);
        System.out.println("Factorial of " + num + " = " + fact);
    }
    public static int fac(int n) {
        int fact = 1;
        for(int i = 1; i <= n; i++) {
            fact *= i;
        }
        return fact;
    }
}
