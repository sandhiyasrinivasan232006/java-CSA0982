public class PerfectNumber {
    public static void main(String[] args) {
        int num = 6;
        int sum = 0;
        for (int i = 1; i < num; i++) {
            if (n % i == 0) {
                sum += i;
            }
        }
        if (sum == num) {
            System.out.println("It's a Perfect Number");
        } else {
            System.out.println("It's not a Perfect Number");
        }
    }
}
